.data 
string: .asciiz "Hello"
a: .word 0
input: .space 50
.text 
.globl main
main:
li $v0, 11
lb $a0, string
syscall

li $v0, 11
lb $a0, string+1
syscall

la $a0, '\n'
li $v0, 11
syscall

li $v0,8
la $a0, input
li $a1, 50
syscall

la $a0, '\n'
li $v0, 11
syscall

li $v0, 11
lb $a0, input+1
syscall

la $a0, '\n'
li $v0, 11
syscall


li $v0, 10 
syscall    # Έξοδος

