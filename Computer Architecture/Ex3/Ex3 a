.data # τμήμα δεδομένων
string: .asciiz "Hello"
input:   .space 50

.text # τμήμα κώδικα
.globl main
main:

#a)
li  $v0, 11           #give string
lb  $a0, string    
syscall


li  $v0, 11
lb  $a0, string+1  
syscall

la $a0, '\n'
li $v0, 11
syscall

#c)

li $v0, 8
la $a0, input
li $a1, 50     # give string
syscall


li  $v0, 11
lb  $a0, input+1
syscall


li $v0, 10 
syscall    # Έξοδος
